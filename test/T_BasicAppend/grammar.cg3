DELIMITERS = "<$.>" ;

"<word>" APPEND ("newword" appended1 @A @B "newword" appended3 @A @C) (*) (NOT 0 (appended1)) ;
"<word>" APPEND ("newword" appended2) (*) (NOT 0 (appended2)) ;

COPY (copy copied) (wanted) - (copy) ;
